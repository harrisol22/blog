<%- include("partials/header") -%>

<div class="row">
  <h1>Compose</h1>
</div>

<form class="" action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input class="form-control" type="text" name="title" autocomplete="off">
    <label>Post</label>
    <textarea class="form-control" name="newPost" rows="8" cols="80"></textarea>
  </div>

  <button class="btn btn-primary" type="submit" name="publish">Publish</button>

</form>


<%- include("partials/footer")-%>
